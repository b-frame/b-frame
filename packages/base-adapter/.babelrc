{
  "presets": [
    ["@babel/preset-env", {
			"bugfixes": true,
			"modules": false,
      "targets": {
				"esmodules": true
      }
    }]
  ],
  "plugins": [
		["module-resolver", {
      "root": ["./src"]
    }],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-private-methods"
  ]
}
